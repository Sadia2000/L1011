<?xml version="1.0"?>

<PropertyList>

<fx>
  <engine>
    <name>left engine</name>
    <path>Aircraft/L1011/Sounds/jet.wav</path>
    <mode>looped</mode>
    <condition>
      <not>
        <property>controls/engines/engine[0]/cutoff</property>
      </not>
    </condition>
    <volume>
      <property>engines/engine[0]/thrust-lbs</property>
      <factor>0.15</factor>
      <min>0.0</min>
      <max>1</max>
    </volume>
    <volume>
      <property>sim/sound/volume</property>
      <value>1</value>
    </volume>
    <position>
      <x>0</x>
      <y>9.8</y>
      <z>-3</z>
    </position>
    <reference-dist>200.0</reference-dist>
    <max-dist>500.0</max-dist>
  </engine>
  <engine>
    <name>left n1</name>
    <path>Aircraft/L1011/Sounds/jet.wav</path>
    <mode>looped</mode>
    <property>engines/engine/n1</property>
    <volume>
      <property>engines/engine[0]/n1</property>
      <factor>0.12</factor>
      <min>0.0</min>
      <max>0.6</max>
    </volume>
    <volume>
      <property>sim/sound/volume</property>
    </volume>
    <pitch>
      <property>/engines/engine[0]/n1</property>
      <offset>0</offset>
      <factor>0.02</factor>
      <min>0.0</min>
      <max>2.0</max>
    </pitch>
    <position>
      <x>0</x>
      <y>-9.8</y>
      <z>-3</z>
    </position>
    <reference-dist>10.0</reference-dist>
    <max-dist>30.0</max-dist>
  </engine>



  <engine>
    <name>right engine</name>
    <path>Aircraft/L1011/Sounds/jet.wav</path>
    <mode>looped</mode>
    <condition>
      <not>
        <property>controls/engines/engine[1]/cutoff</property>
      </not>
    </condition>
    <volume>
      <property>/engines/engine[1]/thrust-lbs</property>
      <factor><factor>0.15</factor></factor>
      <min>0.0</min>
      <max>1.0</max>
    </volume>
    <volume>
      <property>sim/sound/volume</property>
    </volume>
    <position>
      <x>0</x>
      <y>-9.8</y>
      <z>-3</z>
    </position>
    <reference-dist>200.0</reference-dist>
    <max-dist>500.0</max-dist>
  </engine>

  <engine>
    <name>right n1</name>
    <path>Aircraft/L1011/Sounds/jet.wav</path>
    <mode>looped</mode>
    <property>engines/engine[1]/n1</property>
    <volume>
      <property>engines/engine[1]/n1</property>
      <factor>0.12</factor>
      <min>0.0</min>
      <max>0.6</max>
    </volume>
    <volume>
      <property>sim/sound/volume</property>
    </volume>
    <pitch>
      <property>/engines/engine[1]/n1</property>
      <offset>0</offset>
      <factor>0.02</factor>
      <min>0.0</min>
      <max>2.0</max>
    </pitch>
    <position>
      <x>0</x>
      <y>-9.8</y>
      <z>-3</z>
    </position>
    <reference-dist>10.0</reference-dist>
    <max-dist>30.0</max-dist>
  </engine>

  <rumble>
    <name>rumble</name>
    <mode>looped</mode>
    <path>Aircraft/L1011/Sounds/rumble.wav</path>
    <condition>
      <or>
	<property>/gear/gear[0]/wow</property>
	<property>/gear/gear[1]/wow</property>
	<property>/gear/gear[2]/wow</property>
      </or>
    </condition>
		<volume>
			<property>/engines/engine/n1</property>
			<factor type="double">0.0075</factor>
			            <min>0.0</min>
			            <max>1.0</max>
		</volume>
		<pitch>
			<property>/engines/engine/n1</property>
			<type>log</type>
			<factor>0.35</factor>
			            <min>0.0</min>
			            <max>0.75</max>
		</pitch>
  </rumble>

  <gear>
    <name>gear</name>
    <mode>in-transit</mode>
    <path>Aircraft/L1011/Sounds/gear.wav</path>
    <property>/gear/gear[0]/position-norm</property>
    <pitch>
      <internal>dt_play</internal>
      <offset>1.0</offset>
      <factor>-0.001</factor>
    </pitch>
  </gear>

  <gear>
    <name>gear-lock</name>
    <path>Aircraft/L1011/Sounds/gear-lck.wav</path>
    <condition>
      <equals>
	<property>/gear/gear[0]/position-norm</property>
	<value>0</value>
      </equals>
    </condition>
  </gear>
  <!-- 
  <gear>
    <name>gear-horn</name>
    <path>Aircraft/L1011/Sounds/gear-hrn.wav</path>
    <mode>looped</mode>
    <condition>
      <equals>
	<property>/gear/gear[0]/position-norm</property>
	<value>0</value>
      </equals>
    </condition>      
  </gear>
  -->

  <squeal>
    <name>squeal</name>
    <path>Aircraft/L1011/Sounds/squeal.wav</path>
    <condition>
      <or>
	<property>/gear/gear[0]/wow</property>
	<property>/gear/gear[1]/wow</property>
	<property>/gear/gear[2]/wow</property>
      </or>
    </condition>
    <volume>
      <property>/velocities/speed-down-fps</property>
      <factor>0.05</factor>
    </volume>
    <volume>
      <property>/velocities/airspeed-kt</property>
      <factor>0.01</factor>
    </volume>
    <volume>
      <internal>dt_stop</internal>
      <factor>0.05</factor>
      <max>1.0</max>
    </volume>
    <pitch>
      <property>/velocities/airspeed-kt</property>
      <factor>0.0025</factor>
      <offset>1.2</offset>
    </pitch>
  </squeal>
  
  <flap>
    <name>flaps</name>
    <mode>in-transit</mode>
    <path>Aircraft/L1011/Sounds/flaps.wav</path>
    <property>/surface-positions/flap-pos-norm</property>
    <volume>
      <factor>0.45</factor>
    </volume>
  </flap>

  <wind>
    <name>wind</name>
    <mode>looped</mode>
    <path>Aircraft/L1011/Sounds/wind.wav</path>
    <property>/velocities/airspeed-kt</property>
    <volume>
      <property>/position/altitude-ft</property>
      <factor>-0.000015</factor>
      <offset>1.0</offset>
      <min>0.1</min>
      <max>1.0</max>
    </volume>
    <volume>
      <property>/velocities/airspeed-kt</property>
      <factor>0.0015</factor>
      <min>0.03</min>
      <max>0.25</max>
    </volume>
    <pitch>
      <property>/velocities/airspeed-kt</property>
      <factor>0.01</factor>
      <offset>1.25</offset>
    </pitch>
  </wind>

  <overspeed>
    <name>overspeed</name>
    <mode>looped</mode>
    <path>Aircraft/L1011/Sounds/overspeed2.wav</path>
    <condition>
      <greater-than>
	<property>velocities/airspeed-kt</property>
	<value>340</value>
      </greater-than>
      <equals>
	<property>sim/current-view/view-number</property>
	<value>0</value>
      </equals>
    </condition>
    <volume>
      <factor>1.0</factor>
    </volume>
    <pitch>
      <factor>0.1</factor>
    </pitch>
    <reference-dist>1.0</reference-dist>
    <max-dist>2.0</max-dist>
  </overspeed>
  
  <stall>
    <name>stall</name>
    <mode>looped</mode>
    <path>/Aircraft/L1011/Sounds/stall.wav</path>
    <condition>
      <less-than>
        <property>velocities/airspeed-kt</property>
        <value>140</value>
      </less-than>
      <not>
        <property>/gear/gear[0]/wow</property>
        <property>/gear/gear[1]/wow</property>
        <property>/gear/gear[2]/wow</property>
        <property>/surface-positions/flap-pos-norm</property>
        <property>/gear/gear[0]/position-norm</property>
      </not>
      <less-than>
        <property>surface-positions/flap-pos-norm</property>
        <value>.01</value>
      </less-than>
    </condition>
    <volume>
      <max>1.0</max>
    </volume>
    <position>
      <x>-23</x>
      <y>0</y>
      <z>1</z>
    </position>
    <reference-dist>1.0</reference-dist>
    <max-dist>2.0</max-dist>
  </stall>

  <!-- no binding yet?
  <click>
    <name>click</name>
    <path>Aircraft/L1011/Sounds/click.wav</path>
  </click>
  -->

